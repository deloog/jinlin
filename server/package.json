{
  "name": "jinlin-app-server",
  "version": "1.0.0",
  "description": "Jinlin App Server",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "test": "NODE_ENV=test jest --config tests/jest.config.js",
    "test:unit": "NODE_ENV=test jest --config tests/jest.config.js --testMatch='**/tests/unit/**/*.test.js'",
    "test:integration": "NODE_ENV=test jest --config tests/jest.config.js --testMatch='**/tests/integration/**/*.test.js'",
    "test:e2e": "NODE_ENV=test jest --config tests/jest.config.js --testMatch='**/tests/e2e/**/*.test.js' --setupFilesAfterEnv='./tests/e2e/setup.js' --teardown='./tests/e2e/teardown.js'",
    "test:coverage": "NODE_ENV=test jest --config tests/jest.config.js --coverage",
    "test:win": "set NODE_ENV=test&& jest --config tests/jest.config.js",
    "test:win-unit": "set NODE_ENV=test&& jest --config tests/jest.config.js --testMatch='**/tests/unit/**/*.test.js'",
    "test:win-integration": "set NODE_ENV=test&& jest --config tests/jest.config.js --testMatch='**/tests/integration/**/*.test.js'",
    "test:win-e2e": "set NODE_ENV=test&& jest --config tests/jest.config.js --testMatch='**/tests/e2e/**/*.test.js' --setupFilesAfterEnv=./tests/e2e/setup.js --teardown=./tests/e2e/teardown.js",
    "test:win-coverage": "set NODE_ENV=test&& jest --config tests/jest.config.js --coverage",
    "backup": "node scripts/disaster-recovery/backup.js",
    "backup:db": "node scripts/disaster-recovery/backup.js --type=db",
    "backup:files": "node scripts/disaster-recovery/backup.js --type=files",
    "backup:config": "node scripts/disaster-recovery/backup.js --type=config",
    "restore:db": "node scripts/disaster-recovery/database-restore.js",
    "restore:system": "node scripts/disaster-recovery/system-restore.js",
    "test:performance": "node tests/performance/benchmark.js",
    "test:load": "node tests/performance/loadTest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start:test": "NODE_ENV=test node index.js",
    "init-db": "node ./db/init.js",
    "seed-db": "node ./db/seed.js",
    "start:dev": "NODE_ENV=development node index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "start:win-dev": "set NODE_ENV=development&& node index.js",
    "start:win-test": "set NODE_ENV=test&& node index.js",
    "start:win-prod": "set NODE_ENV=production&& node index.js",
    "migrate": "knex migrate:latest",
    "migrate:dev": "NODE_ENV=development knex migrate:latest",
    "migrate:test": "NODE_ENV=test knex migrate:latest",
    "migrate:prod": "NODE_ENV=production knex migrate:latest",
    "migrate:win-dev": "set NODE_ENV=development&& knex migrate:latest",
    "migrate:win-test": "set NODE_ENV=test&& knex migrate:latest",
    "migrate:win-prod": "set NODE_ENV=production&& knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:dev": "NODE_ENV=development knex seed:run",
    "seed:test": "NODE_ENV=test knex seed:run",
    "seed:prod": "NODE_ENV=production knex seed:run",
    "seed:win-dev": "set NODE_ENV=development&& knex seed:run",
    "seed:win-test": "set NODE_ENV=test&& knex seed:run",
    "seed:win-prod": "set NODE_ENV=production&& knex seed:run",
    "seed:make": "knex seed:make",
    "db:migrate": "node ./db/migrate.js migrate",
    "db:rollback": "node ./db/migrate.js rollback",
    "db:rollback-all": "node ./db/migrate.js rollback all",
    "db:status": "node ./db/migrate.js status",
    "db:seed": "node ./db/migrate.js seed",
    "benchmark": "node ./benchmark/benchmark.js",
    "profile:doctor": "clinic doctor -- node index.js",
    "profile:flame": "clinic flame -- node index.js",
    "profile:bubbleprof": "clinic bubbleprof -- node index.js",
    "profile:heap": "clinic heapprofiler -- node index.js",
    "profile:all": "node ./benchmark/profile.js",
    "autocannon": "autocannon -c 100 -d 10 http://localhost:3000/health"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^8.0.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.48",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.3",
    "querystring": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "",
  "license": "ISC"
}
